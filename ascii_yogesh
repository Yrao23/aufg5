hohe_input = int(input("hÃ¶he: "))
breite_input = int(input("breite: "))

hohe = int(hohe_input)
breite_line1 = int(breite_input) + 2
breite_line2 = int(breite_input) + 1
    
    
for x in range(1, breite_line1, 1):
    print("C", end='')
